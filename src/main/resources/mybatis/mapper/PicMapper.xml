<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.gaodedemo.mapper.PicMapper">
<insert id="insertPic" parameterType="com.example.gaodedemo.entity.PicDO">
    INSERT INTO pic(uuid,class_id,title,pic_link)VALUES (#{uuid},#{classId},#{title},#{picLink})
</insert>
</mapper>